<div class="rounded_container login_form">
  <%= form_for :login, url:'/sessions' do |f| %>
    <div class="field">
      <%= f.label :email_address  %>
      <%= f.email_field :email_address, size: 25 %>
    </div>
    <div class="form-group">
      <%= f.label :password %>
      <%= f.password_field :password, size: 25 %>
    </div>
    <div class="actions">
      <%= f.submit 'Login' %>
    </div>
  <% end %>
</div>
